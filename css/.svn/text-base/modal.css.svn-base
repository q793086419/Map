}
.star {
    display: inline-block;
    cursor:pointer;
    width: 16px;
    height: 16px;
    background: url("../images/modal-icon.png") no-repeat -37px 0px;
    margin-right: 9px;
}
.star-click {
    background-position: -55px 0px;
}
.modal .close{
	margin: 0;
	position: absolute;
	top:-3px;
	right:0;
	z-index: 200;
	color:#999;
}
.modalBtn:hover{
	background: #ec8218;
	color:#ffffff;
}

