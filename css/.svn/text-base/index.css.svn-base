html, body {
    margin: 0;
    padding: 0;
    font-family: "Microsoft YaHei";
}
html {
    width: 100%;
    height: 100%;
}
body {
    font-size: 12px;
    background-color: #fff;
    margin: 0;
    color: #4c4c4c;
    padding: 0;
    height: 100%;
    width: 100%;
    overflow: hidden;
    -ms-touch-action: none;
    touch-action: none;
}
#app {
    width: 100%;
    height: 100%;
    position: relative;
    overflow: hidden;
}
#maps {
    width: 100%;
    height: 100%;
}
#left-panel1 {
    position: fixed;
    left: 0;
    top: 60px;
    overflow: hidden;
    z-index: 100;
    transition-property:top;
    transition-duration:0.5s;
    transition-timing-function:ease;
}
#left-panel1 .preson-list,#left-panel1 .preson-nav{
	width:323px;
	height: calc(100% - 38px);
}
#left-panel1 .preson-list{
	height: calc(100% - 129px);
	overflow: auto;
}
#left-panel1 .preson-nav{
	position: absolute;
	top:calc(-100% - 38px);
	transition-property:top;
    transition-duration:0.5s;
    transition-timing-function:ease;
    background: #ffffff;
/*     z-index: 200; */
}
#left-panel1.preson-nav-down .preson-nav{
	top:38px;
}
.preson-item-header{
	position: relative;
	z-index: 200;
	background: #fff;
}
#left-panel2 {
    position: fixed;
    left: 0;
    top: 60px;
    overflow: hidden;
    z-index: 100;
    transition-property:top;
    transition-duration:0.5s;
    transition-timing-function:ease;
}
.navbar{
	margin-bottom: 0;
}
.navbar-min{
	background: url("../images/nav-bg.png");
    opacity:0.9;
    border-radius: 0;
    border:0;
    position: fixed;
    height: 60px;
    width: 100%;
    z-index: 100;
    top:0;
    transition-property:top;
    transition-duration:0.5s;
    transition-timing-function:ease;
}
.navbar-brand{
	color: #fff;
	height: 60px;
	width: 305px;
}
.navbar-brand:hover,.navbar-brand:focus{
	color:#fff;
}
.navbar-nav>li>a{
	color:#fff;
}
.nav>li>a:focus, .nav>li>a:hover{
	background-color: transparent;
}
.nav .open>a, .nav .open>a:focus, .nav .open>a:hover{
	background-color: transparent;
}
.navbar-nav>li>a{
	padding-top: 0;
	padding-bottom: 0;
	padding: 9px 57px;
	color: #a0b3d4;
	height: 57px;
}
.navbar-nav>li>a:hover{
	background: none;
}
.navbar-nav>li:hover{
	background: #52627b;
	border-top: 3px solid #058cf4;
}
.navbar-nav>li:hover a{
	color:#fff;
}
.navbar-nav>li,.navbar-nav>li a{
	border-top: 3px solid rgba(0,0,0,0);
	border-color: transparent;
}
.navbar-nav>li.active>a{
	color:#fff;
	background: #52627b;
}
.navbar-nav>li.active{
	border-top: 3px solid #058cf4;
}

.nav-icon{
	display: block;
	width: 22px;
	height: 22px;
	background: url("../images/nav.png");
	margin: 0 auto;
}
.navbar-nav>li.active i,.navbar-nav>li:hover i{
	background-position-y:22px; 
}
.nav-icon-j{
	background-position: 0px 0px;
}
.nav-icon-g{
	background-position: -22px 0px;
}
.nav-icon-t{
	background-position: -44px 0px;
}
.nav-icon-q{
	background-position: 44px 0px;
}
.navbar-right li,.navbar-right li:hover{
	border: 0;
	color: #fff;
}
.navbar-right li i{
	display: inline-block;
	vertical-align: text-bottom;
}
.navbar-right>li>a{
	height: 60px;
	line-height: 40px;
	padding: 9px 23px;
}
.nav-icon-h{
	background: url("../images/nav-h.png");
	width: 18px;
    height: 18px;
}
.navbar-right>li:hover i{
	background-position-y: initial;
}
.navbar-right>li:hover a{
	color: #fff;
}
.navbar-right>li:hover{
	background: none;
}
